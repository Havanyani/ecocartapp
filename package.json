    "prebuild-android": "npx expo prebuild --platform android --clean",
    "build-ios": "npx eas build --platform ios --profile production",
    "build-android": "npx eas build --platform android --profile production",
    "ci-check": "node scripts/ci-local-check.js",
    "start-minimal": "node start-minimal.js",
    "metro-minimal": "node metro-start.js",
    "start-commonjs": "node run-commonjs.js",
