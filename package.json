  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "ios-fix": "scripts\\ios-fix-connection.bat",
    "ios-battery-fix": "node scripts/fix-ios-battery.js && npm run qr-launch",
