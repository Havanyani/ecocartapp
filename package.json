    "fix-and-start": "npm run clear-cache && npm run patch-cli && npm run web-fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:unit": "jest --testPathIgnorePatterns=\"tests/integration/.*\"",
    "test:integration": "jest \"tests/integration/.*\"",
    "e2e:build:android": "detox build --configuration android.emu.debug",
    "e2e:build:ios": "detox build --configuration ios.sim.debug",
    "e2e:test:android": "detox test --configuration android.emu.debug",
    "e2e:test:ios": "detox test --configuration ios.sim.debug",
    "e2e:test": "detox test",
    "start-all": "node scripts/platform-launcher.js",
    "start-cmd": "node scripts/cmd-launcher.js",
    "web-basic": "set EXPO_PUBLIC_PLATFORM=web && set EXPO_DEBUG_NO_ROUTER=true && set EXPO_DEBUG_WEB_ONLY=true && expo start --web --no-dev",
    "platform-menu": "node scripts/platform-menu.js",
    "clean-start": "npx expo start -c",
    "reset-app": "npm cache clean --force && rmdir /s /q node_modules\\.cache && npm run clean-start",
    "debug-mode": "set EXPO_DEBUG_MODE=1 && npx expo start -c --port 19001",
    "tunnel": "scripts\\tunnel-launch.bat",
    "tunnel-enhanced": "scripts\\tunnel-enhanced.bat",
    "app-store-build": "scripts\\create-app-store-build.bat",
